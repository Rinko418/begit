{
	"name": "@begit/core",
	"version": "0.0.17",
	"description": "Blazingly fast tool for cloning git repositories, with no reliance on local `git`, or `tar` installs",
	"author": "Thomas Beer",
	"license": "MIT",
	"homepage": "https://github.com/Tommypop2/begit",
	"repository": {
		"type": "git",
		"url": "https://github.com/Tommypop2/begit"
	},
	"type": "module",
	"files": ["dist", "types"],
	"exports": {
		".": {
			"types": "./types/index.d.ts",
			"import": "./dist/index.js",
			"default": "./dist/index.js"
		}
	},
	"scripts": {
		"test": "vitest",
		"build": "tsc && tsup"
	},
	"keywords": ["git", "template", "scaffolding"],
	"devDependencies": {
		"@types/node": "^20.14.2",
		"@types/tar": "^6.1.13",
		"tsup": "^8.1.0",
		"typescript": "^5.4.5",
		"vitest": "^1.6.0"
	},
	"dependencies": {
		"tar": "^7.2.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
